[sub_resource type="Shader" id="Shader_tq47i"]
code = "shader_type canvas_item;

uniform sampler2D SCREEN_TEXTRURE: hint_screen_texture;
uniform float alpha: hint_range(0.0, 1.0, 0.05) = 1.0;
uniform float brightness: hint_range(-1.0, 1.0, 0.05);
uniform vec2 offset;

//void vertex() {
	//VERTEX += offset;
//}

void fragment(){
	vec4 tex = texture(SCREEN_TEXTRURE, SCREEN_UV - offset);
	tex.rgb *= 1.0 + vec3(brightness);
	tex.a *= alpha;
	COLOR = tex;
}"

